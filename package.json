{
  "name": "multicolour",
  "version": "0.1.0",
  "description": "Generate your API and single page app with a config file and JSON and use almost any DB tech",
  "main": "index.js",
  "preferGlobal": "true",
  "bin": {
    "multicolour": "start.js"
  },
  "scripts": {
    "test": "tape tests/*.js | tap-spec",
    "test-coverage": "istanbul cover tape tests/*.js --recursive --report lcov html -x cli.js | tap-spec",
    "watch-tests": "nodemon -x \"clear && npm test\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/newworldcode/multicolour.git"
  },
  "author": "newworldcode <hello@newworld.codes> (https://newworld.codes)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/newworldcode/multicolour/issues"
  },
  "homepage": "https://github.com/newworldcode/multicolour#readme",
  "dependencies": {
    "@newworldcode/talkie": "^1.0.0",
    "commander": "^2.8.1",
    "fs-extra": "^0.24.0",
    "inquirer": "^0.10.0",
    "pluralize": "^1.2.1",
    "replacestream": "^4.0.0",
    "uuid": "^2.0.1",
    "validate-npm-package-name": "^2.2.2",
    "waterline": "^0.10.26"
  },
  "engine-strict": true,
  "engineStrict": true,
  "engines": {
    "node": ">= 4.0.0"
  },
  "devDependencies": {
    "eslint": "^1.5.1",
    "istanbul": "^0.3.22",
    "nodemon": "^1.7.1",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.0"
  }
}
