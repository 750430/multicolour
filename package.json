{
  "name": "multicolour",
  "version": "0.4.0",
  "description": "Generate your API and single page app with a config file and JSON and use almost any DB tech",
  "main": "index.js",
  "preferGlobal": "true",
  "bin": {
    "multicolour": "start.js"
  },
  "scripts": {
    "test": "snyk test && tape tests/*.js | tap-spec",
    "test-coverage": "istanbul cover tape tests/*.js --recursive --report lcov html -x cli.js | tap-spec",
    "changelog": "github_changelog_generator -u Multicolour -p multicolour && git add CHANGELOG.md && git commit -m \"Autogenerated :boom:\" && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Multicolour/multicolour.git"
  },
  "author": "newworldcode <hello@newworld.codes> (https://newworld.codes)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Multicolour/multicolour/issues"
  },
  "homepage": "https://github.com/Multicolour/multicolour#readme",
  "dependencies": {
    "@newworldcode/talkie": "^1.0.9",
    "async": "^2.0.0",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "fs-extra": "^0.30.0",
    "inquirer": "^1.1.2",
    "jsocrud": "^2.0.3",
    "pluralize": "^3.0.0",
    "replacestream": "^4.0.0",
    "snyk": "^1.17.0",
    "uuid": "^2.0.2",
    "validate-npm-package-name": "^2.2.2",
    "waterline": "^0.12.2"
  },
  "engine-strict": true,
  "engineStrict": true,
  "engines": {
    "node": ">= 4.0.0"
  },
  "devDependencies": {
    "eslint": "^3.0.1",
    "istanbul": "^0.4.4",
    "mkdirp": "^0.5.1",
    "sails-memory": "^0.10.7",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0"
  }
}
